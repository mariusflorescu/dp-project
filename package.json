{
  "name": "ecommerce",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "format": "yarn prettier --write .",
    "db:up": "docker-compose up -d",
    "migrate:dev": "dotenv -e .env.local prisma migrate dev",
    "db-push:dev": "dotenv -e .env.local prisma db push",
    "dx": "yarn run db:up && yarn run migrate:dev",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@mantine/core": "^4.1.3",
    "@mantine/form": "^4.1.4",
    "@mantine/hooks": "^4.1.3",
    "@mantine/next": "^4.1.3",
    "@mantine/notifications": "^4.1.3",
    "@next-auth/prisma-adapter": "^1.0.3",
    "@prisma/client": "^3.12.0",
    "axios": "^0.26.1",
    "next": "12.1.5",
    "next-auth": "^4.3.2",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "swr": "^1.3.0",
    "tabler-icons-react": "^1.45.0"
  },
  "devDependencies": {
    "@types/node": "17.0.24",
    "@types/react": "18.0.5",
    "@types/react-dom": "18.0.1",
    "eslint": "8.13.0",
    "eslint-config-next": "12.1.5",
    "prettier": "^2.6.2",
    "prisma": "^3.12.0",
    "typescript": "4.6.3"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  }
